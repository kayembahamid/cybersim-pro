{
  "name": "cybersim-pro-mcp",
  "version": "1.1.0",
  "description": "Professional cybersecurity training and simulation MCP server",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "cybersim-pro": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "npm run build && node build/index.js",
    "serve:http": "npm run build && node build/httpServer.js",
    "audit:seal": "npm run build && node build/cli/exportAuditSeal.js",
    "compliance:report": "npm run build && node build/cli/complianceReport.js",
    "migrate:provenance": "npm run build && node build/cli/backfillProvenance.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "cybersecurity",
    "simulation",
    "training",
    "security",
    "threat-intelligence",
    "incident-response",
    "forensics"
  ],
